{
  "name": "sq-core",
  "version": "1.0.0",
  "description": "Full stack core framework",
  "main": "index.js",
  "scripts": {
    "client:test": "node scripts/test.js --no-watch",
    "client:test:watch": "node scripts/test.js",
    "client:test:coverage": "node scripts/test.js --no-watch --coverage",
    "server:test": "mocha --file ./tests/setup.js  --exit --recursive ./server/**/**",
    "server:test:coverage": "nyc --reporter=html --reporter=lcov --reporter=text mocha --file ./tests/setup.js  --exit --recursive ./server/**/**",
    "server:test:watch": "mocha  -w --recursive ./server/**/**",
    "test": "npm run server:test && npm run client:test",
    "test:coverage": "npm run server:test:coverage && npm run client:test:coverage",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "nyc": {
    "all": true,
    "include": [
      "server/**/**/*.js"
    ],
    "exclude": [
      "server/**/*spec.js",
      "server/test/**/*.js",
      "server/mock/**/*.js"
    ],
    "report-dir": "./coverage/server"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
